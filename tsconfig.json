{
  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "ES5",
    "outDir": "./lib",
    "allowJs": true,
    "lib": [
      "ES5",
      "ES2015",
      "ES2015.Promise",
      "DOM",
      "ES2015.Collection",
      "ES2016",
      "ES6"
    ],
    "types": []
  },
  "include": ["src/**/*"]
}
